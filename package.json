{
    "name": "fuwa.js",
    "version": "0.1.0",
    "description": "A new and powerful discord bot library; inspired by express",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "fuwa",
        "fuwajs",
        "discord",
        "discordapi",
        "discordbot"
    ],
    "scripts": {
        "docs": "npx typedoc --options ./docs.js",
        "test": "npm run build && npx nodemon examples/index",
        "browser": "tsc -p . && node browser",
        "profile": "npm run build && node --prof examples/index",
        "build": "npx tsc -p ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Fuwajs/Fuwa.js.git"
    },
    "contributors": [
        "Artrix",
        "Phoenix"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Fuwajs/Fuwa.js/issues"
    },
    "homepage": "https://github.com/Fuwajs/Fuwa.js#readme",
    "dependencies": {
        "chalk": "^2.4.2",
        "erlpack": "*",
        "undici": "^4.5.1",
        "ws": "^8.2.1"
    },
    "devDependencies": {
        "@types/node": "^14.14.13",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@typescript-eslint/parser": "^4.12.0",
        "husky": "^4.3.6",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1",
        "typedoc": "^0.20.13",
        "typescript": "^4.1.3"
    },
    "optionalDependencies": {
        "chalk": "^2.4.2",
        "erlpack": "*"
    },
    "lint-staged": {
        "*.--write": "prettier --write"
    }
}
